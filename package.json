    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.5.3"
  },
  "bugs": {
    "url": "https://github.com/tjdev7/FontsPanel/issues"
  }
